{
  "name": "doggo-sports",
  "version": "0.0.0",
  "author": "Anthony Shew",
  "scripts": {
    "dev": "docker-compose -f docker-compose.yml -f docker-compose.dev.yml up",
    "dev:build": "docker-compose -f docker-compose.yml -f docker-compose.dev.yml up --build",
    "dev:down": "docker-compose down",
    "test": "docker-compose -f docker-compose.yml -f docker-compose.test.yml up",
    "test:build": "docker-compose -f docker-compose.yml -f docker-compose.test.yml up --build --exit-code-from cypress",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run"
  },
  "devDependencies": {
    "@types/node": "^17.0.8",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.4",
    "cypress": "^9.2.1"
  },
  "dependencies": {}
}
